<script>
function pagegogo(g){
	//페이지 이동 
	//alert($(g).text());
	if(page==$(g).text()){
		return;
	}
	$('.go1').removeClass('page-li page-item go1 active').addClass('page-li page-item go1');
	$(g).closest('.go1').addClass('page-li page-item go1 active');
	var sss="pro"+$(g).text();
	var ssd="pro"+page;
	
	
	
	
	$("#"+sss).css("display","");
	$("#"+ssd).css("display","none");
	
	page=$(g).text();
	
}
</script>

