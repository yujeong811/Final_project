<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Review-Mapper">
	<select id="selectSub" resultType="Subject">
		select *
		from subject
		where subject_code > 0
	</select>	
</mapper>
